{
  "version": "1.0.0",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": false,
    "compilerName": "fsc",
    "compile": {
      "includeFiles": [
        "Fable.Plugins.NUnit.fsx"
      ]
    },
    "define": [
      "DOTNETCORE",
      "TRACE"
    ],
    "nowarn": [
      "44",
      "2003"
    ],
    "xmlDoc": true,
    "delaySign": true,
    "warningsAsErrors": true,
    "additionalArguments": [
      "--fullpaths",
      "--flaterrors"
    ]
  },
  "dependencies": {
    "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-*",
    "Fable.Core": {
      "version": "1.0.0-*",
      "target": "project"
    },
    "Fable.Compiler": {
      "version": "1.0.0-*",
      "target": "project"
    }
  },
  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-*",
      "imports": "dnxcore50"
    }
  },
  "frameworks": {
    "netstandard1.6": {
      "NETStandard.Library": "1.6.0"
    }
  }
}